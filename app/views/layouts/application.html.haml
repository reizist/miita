!!! 5

%html

  %head
    %title DocShare 
    = stylesheet_link_tag "application", :media => "all"
    = stylesheet_link_tag "application", 'https://fonts.googleapis.com/css?family=Josefin+Slab', :media => "all"
    = javascript_include_tag "application"
    = csrf_meta_tags
  %body
    = render 'layouts/navbar'
    - unless user_signed_in?
      = render 'layouts/header'
    - else
      = yield
